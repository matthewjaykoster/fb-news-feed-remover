{
    "manifest_version": 2,
    "name": "FB News Feed Remover",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*.facebook.com/*"
            ],
            "js": [
                "removeNewsFeed.js"
            ]
        }
    ],
    "icons": {
        "48": "48.png",
        "128": "128.png"
    }
}